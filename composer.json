{
    "name": "phlak/stash",
    "description": "Simple PHP caching library",
    "license": "MIT",
    "authors": [
        {
            "name": "Chris Kankiewicz",
            "email": "Chris@ChrisKankiewicz.com"
        }
    ],
    "support": {
        "issues": "https://github.com/PHLAK/Stash/issues"
    },
    "require": {
        "php": ">=5.6",
        "nesbot/carbon": "^1.21"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "friendsofphp/php-cs-fixer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "Stash\\": "src/Stash/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/",
            "tests/traits/"
        ]
    }
}
